{
  "name": "oremus-web",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc --noEmit",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --config jest.integration.config.js",
    "test:auth": "jest auth",
    "test:mass-intentions": "jest mass-intention",
    "auto-supabase": "tsx scripts/auto-supabase.ts",
    "auto-supabase:sync": "npm run auto-supabase sync",
    "auto-supabase:types": "npm run auto-supabase types",
    "auto-supabase:services": "npm run auto-supabase services",
    "auto-supabase:report": "npm run auto-supabase report",
    "migrate-to-auto-supabase": "tsx scripts/migrate-to-auto-supabase.ts",
    "db:start": "supabase start",
    "db:stop": "supabase stop",
    "db:reset": "supabase db reset",
    "db:status": "supabase status",
    "db:diff": "supabase db diff",
    "db:push": "supabase db push",
    "db:pull": "supabase db pull",
    "db:migrate": "supabase migration new",
    "db:seed": "supabase seed run",
    "types:generate": "supabase gen types typescript --project-id $PROJECT_ID > types/supabase-generated.ts",
    "types:watch": "nodemon --watch supabase/migrations --exec npm run types:generate",
    "types:validate": "tsc --noEmit types/supabase-generated.ts",
    "dev:full": "concurrently \"npm run db:start\" \"npm run dev\" \"npm run types:watch\"",
    "dev:reset": "npm run db:reset && npm run types:generate && npm run dev",
    "dev:fresh": "npm run db:stop && npm run db:start && npm run db:reset && npm run auto-supabase:sync && npm run dev",
    "cleanup:types": "rm -rf types/generated && npm run auto-supabase:types",
    "cleanup:cache": "rm -rf .next && npm run dev",
    "cleanup:db": "npm run db:reset && npm run db:seed",
    "cleanup:all": "npm run cleanup:cache && npm run cleanup:types && npm run cleanup:db",
    "validate:migrations": "supabase db reset",
    "validate:types": "tsc --noEmit --skipLibCheck",
    "validate:services": "npm run validate:types && npm run test:integration",
    "validate:all": "npm run validate:migrations && npm run validate:services",
    "deploy:staging": "npm run build && npm run db:push",
    "deploy:production": "npm run test && npm run build",
    "deploy:types": "npm run types:generate && git add types/ && git commit -m 'Update types' && git push",
    "db:types": "npx supabase@latest gen types typescript --local > types/supabase.ts",
    "db:schema:extract": "ts-node --transpile-only tools/schema-extractor.ts",
    "db:schema:detect": "ts-node --transpile-only tools/detect-table-usage.ts",
    "db:schema:update": "ts-node --transpile-only tools/supabase-schema-manager.ts",
    "db:erd": "npx supabase@latest db diagram -o docs/diagrams/database-erd.png",
    "db:detect-tables": "node tools/detect-supabase-tables.js",
    "db:check-multi-tenant": "ts-node --transpile-only tools/check-multi-tenant-schema.ts",
    "db:validate-migrations": "ts-node --transpile-only tools/validate-migrations.ts",
    "db:update-with-erd": "powershell -File tools/update-schema-with-erd.ps1",
    "db:validate-all": "powershell -File tools/validate-and-update-migrations.ps1",
    "db:analyze": "ts-node --transpile-only tools/oremus-project-scanner.ts",
    "db:auto-deploy": "ts-node --transpile-only tools/supabase-schema-auto-manager.ts deploy",
    "db:watch": "ts-node --transpile-only tools/supabase-schema-auto-manager.ts watch",
    "db:validate-auto": "ts-node --transpile-only tools/supabase-schema-auto-manager.ts validate",
    "db:backup": "ts-node --transpile-only tools/supabase-schema-auto-manager.ts backup",
    "db:complete": "npm run db:analyze && npm run db:auto-deploy",
    "db:full-analysis": "npm run db:analyze && npm run db:schema:detect && npm run db:check-multi-tenant"
  },
  "dependencies": {
    "@chakra-ui/react": "^3.21.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@heroicons/react": "^2.0.18",
    "@hookform/resolvers": "^3.3.2",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.5",
    "@stripe/react-stripe-js": "^3.7.0",
    "@stripe/stripe-js": "^2.4.0",
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.50.0",
    "@tanstack/react-query": "^5.17.19",
    "@types/glob": "^8.1.0",
    "@types/nodemailer": "^6.4.14",
    "@upstash/redis": "^1.35.0",
    "@vercel/edge-config": "^1.4.0",
    "chart.js": "^4.5.0",
    "chokidar": "^4.0.3",
    "class-variance-authority": "^0.7.1",
    "client-only": "^0.0.1",
    "clsx": "^2.1.1",
    "cron-parser": "^5.3.0",
    "date-fns": "^3.3.1",
    "exceljs": "^4.4.0",
    "framer-motion": "^11.18.2",
    "lucide-react": "^0.525.0",
    "luckyexcel": "^1.0.1",
    "micro-cors": "^0.1.1",
    "next": "^15.3.4",
    "nodemailer": "^6.9.9",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.3.0",
    "react-day-picker": "^8.10.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.49.3",
    "react-hot-toast": "^2.5.2",
    "react-toastify": "^11.0.5",
    "stripe": "^14.25.0",
    "styled-jsx": "^5.1.7",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "xlsx": "^0.18.5",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@babel/core": "^7.23.9",
    "@babel/preset-env": "^7.23.9",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@continuedev/cli": "^0.0.17",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.17",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@types/xlsx": "^0.0.35",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "autoprefixer": "^10.4.17",
    "concurrently": "^9.2.0",
    "eslint": "^8.56.0",
    "eslint-config-next": "14.1.0",
    "glob": "^11.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "nodemon": "^3.0.0",
    "postcss": "^8.4.35",
    "sql-lint": "^1.0.2",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.0.0",
    "typescript": "^5.3.3",
    "vitest": "^3.2.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
